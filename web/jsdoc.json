{
    "source": {
        "include": ["web/src/js"],
        "includePattern": ".js$"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "jsdoc",
        "template": "node_modules/better-docs"
    }
}